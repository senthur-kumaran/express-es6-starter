{
    "parser": "@babel/eslint-parser",
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "prettier",
        "airbnb-base"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error",
        "max-len": [2, 120],
        "no-console": "off"
    }
}
